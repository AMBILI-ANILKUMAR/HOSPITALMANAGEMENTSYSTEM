@model IEnumerable<HOSPITALMANAGEMENTSYSTEM.Models.Doctors>

@{
    ViewBag.Title = "ViewDoctor";
    Layout = "~/Views/Admin/AdminMaster.cshtml";
}

<table class="table table-hover">
    <tr>
        <th>
            DOCTOR ID
        </th>
        <th>
            DOCTOR NAME
        </th>
        <th>
            GENDER
        </th>
        <th>
            ADDRESS
        </th>

        <th>
            PHONE NUMBER
        </th>
        <th>
            AGE
        </th>
        <th>
            SPECIALIZATION
        </th>
        <th>
            ROLE
        </th>
        <th>
            EMAIL
        </th>
        <th>
            PASSWORD
        </th>
        
    </tr>


    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.DoctId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DoctName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Gender)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Address)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.phonenumber)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.age)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.specializationName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.role)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.password)
    </td>

</tr>
    }

</table>







